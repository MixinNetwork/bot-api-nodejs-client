  "keywords": [
    "mixin",
    "node",
    "blockchain",
    "crypto",
    "js"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "build:umd": "tsdx build --format umd",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --write . && eslint 'src/**' --fix && tsdx lint"
    }
  },
  "author": "contact@mixin.one",
