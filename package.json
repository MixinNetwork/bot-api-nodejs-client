    },
    {
      "path": "dist/mixin-node-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/eslint": "^8.4.2",
