  "license": "MIT",
  "description": "Mixin SDK for Node.js and Javascript",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MixinNetwork/bot-api-nodejs-client.git"
  },
  "bugs": {
    "url": "https://github.com/MixinNetwork/bot-api-nodejs-client/issues"
  },
