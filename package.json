    "@types/bn.js": "^5.1.0",
    "@types/ed2curve": "^0.2.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node-forge": "^1.0.2",
    "@types/serialize-javascript": "^5.0.2",
    "@types/uuid": "^8.3.4",
    "axios": "^0.27.2",
    "bn.js": "^5.2.1",
    "curve25519-js": "^0.0.4",
    "ed2curve": "^0.3.0",
    "ethers": "^5.6.8",
    "int64-buffer": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "jssha": "^3.2.0",
    "lodash.merge": "^4.6.2",
    "node-forge": "^1.3.1",
    "pako": "^2.0.4",
    "serialize-javascript": "^6.0.0",
    "sha3": "^2.1.4",
    "uuid": "^8.3.2",
    "ws": "^8.7.0"
